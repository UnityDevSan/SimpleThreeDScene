datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

model User {
  id        String   @id() @default(cuid())
  name      String?
  email     String?  @unique()
  password  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt()
  testProp  Boolean  @default(false)
  testProp2 String?  @default("test")
}
